<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="338.0" prefWidth="277.0"
	xmlns="http://javafx.com/javafx/20.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="ViewModel.PTFManagerController">
	<children>
		<Label fx:id="mainLabel" text="Add new PTF6N1 or PTF4N4 module." />
		<TextField fx:id="textField" />
		<Button fx:id="addPTF6N1btn" text="Add PTF6N1 module" />
		<Button fx:id="addPTF4N4btn" text="Add PTF4N4 module" />
		<Button fx:id="deleteButton" text="Delete module" />
		<Label fx:id="label" />
		<ListView fx:id="list" prefHeight="200.0" prefWidth="200.0" />
		<Button fx:id="returnButton" text="Return" />
	</children>
</VBox>
